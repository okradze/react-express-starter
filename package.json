        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
    "dependencies": {
        "express": "^4.17.1",
        "mongoose": "^5.7.5"
    },
    "devDependencies": {
        "concurrently": "^4.1.0",
        "env-cmd": "^9.0.2",
        "nodemon": "^1.19.1"
    }
}
